{
  "name": "claims_platform",
  "version": "1.0.0",
  "description": "Backend MVC Express + TS + Sequelize for denuncias_app schema",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "start": "node --enable-source-maps dist/app.js",
    "build": "tsc -p tsconfig.json",
    "db:init": "tsx scripts/init-db.ts",
    "db:migrate": "tsx scripts/run-migrations.ts"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.5",
    "on-finished": "^2.4.1",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/on-finished": "^2.3.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
